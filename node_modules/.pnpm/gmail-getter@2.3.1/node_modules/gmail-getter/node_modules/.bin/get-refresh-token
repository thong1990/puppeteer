#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/futhong/puppeteer/node_modules/.pnpm/gmail-getter@2.3.1/node_modules/gmail-getter/dist/node_modules:/home/futhong/puppeteer/node_modules/.pnpm/gmail-getter@2.3.1/node_modules/gmail-getter/node_modules:/home/futhong/puppeteer/node_modules/.pnpm/gmail-getter@2.3.1/node_modules:/home/futhong/puppeteer/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/futhong/puppeteer/node_modules/.pnpm/gmail-getter@2.3.1/node_modules/gmail-getter/dist/node_modules:/home/futhong/puppeteer/node_modules/.pnpm/gmail-getter@2.3.1/node_modules/gmail-getter/node_modules:/home/futhong/puppeteer/node_modules/.pnpm/gmail-getter@2.3.1/node_modules:/home/futhong/puppeteer/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/get-token-script.js" "$@"
else
  exec node  "$basedir/../../dist/get-token-script.js" "$@"
fi
